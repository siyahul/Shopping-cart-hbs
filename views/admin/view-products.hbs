<section>
    <div class="container">
        <div class="row mt-4">
            <a href="/admin/add-products" class="btn btn-primary ml-auto">Add Product</a>
        </div>
        <table class="table mt-5">
  <thead>
    <tr>
      <th scope="col">No.</th>
      <th scope="col">Name</th>
      <th scope="col">Price</th>
      <th scope="col">Description</th>
      <th scope="col">Catogory</th>
      <th scope="col">Image</th>
    </tr>
  </thead>
  <tbody>
      {{#each products}}
    <tr>
      <th scope="row">1</th>
      <td>{{this.name}}</td>
      <td>{{this.price}}</td>
      <td>{{this.description}}</td>
      <td>{{this.catogory}}</td>
      <td><img style="width:50px;height:50px;object-fit:contain" src="{{this.image}}"</td>
    </tr>
    {{/each }}
  </tbody>
</table>
    </div>
</section>